{
  "name": "materia-eco",
  "version": "0.0.1",
  "scripts": {
    "build": "yarn generate && next build",
    "clean": "del-cli .next",
    "dev": "next",
    "start": "next start",
    "format": "sort-package-json; pretty-quick --staged",
    "generate": "graphql-codegen --config graphql.config.yaml -r dotenv/config"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run format"
    }
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "graphql-request": "^3.4.0",
    "next": "^10.0.7",
    "react": "^17.0.1",
    "react-datocms": "^1.5.6",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@cprecioso/tsconfig": "^1.0.0",
    "@fortawesome/fontawesome-common-types": "^0.2.34",
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/typed-document-node": "^1.18.4",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@types/node": "^14.14.30",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "del-cli": "^3.0.1",
    "dotenv": "^8.2.0",
    "graphql": "^15.5.0",
    "husky": "^5.0.9",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "sort-package-json": "^1.49.0",
    "typescript": "^4.1.5"
  }
